{
  "name": "artemisa",
  "version": "1.0.2-alpha-04",
  "description": "React/Redux data fetching API",
  "license": "MIT",
  "author": "Javier Fernandes",
  "repository": {
    "type": "git",
    "url": "javierfernandes/artemisajs"
  },
  "scripts": {
    "build": "babel index.babel.js --out-file index.js && babel src --out-dir lib",
    "prepublish": "npm run build",
    "test": "export NODE_PATH=./src && mocha --require jsdom-global/register --compilers js:babel-register --recursive 'test/**/*.@(js|jsx)'",
    "test:watch": "npm run test -- --watch",
    "lint": "eslint src/ test/",
    "coverage-test": "export NODE_PATH=./src && NODE_ENV=test nyc --require babel-core/register mocha --require jsdom-global/register --compilers js:babel-register --recursive 'test/**/*.@(js|jsx)'",
    "coverage-report": "nyc report --reporter=lcov",
    "coverage-upload": "codecov"
  },
  "devDependencies": {
    "babel-cli": "^6.0.0",
    "babel-core": "^6.7.6",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-array-includes": "^2.0.3",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "codecov": "^2.1.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^2.10.2",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-import": "^1.8.0",
    "eslint-plugin-jsx-a11y": "^1.2.2",
    "eslint-plugin-react": "^5.1.1",
    "expect": "^1.20.2",
    "jsdom": "^9.8.3",
    "jsdom-global": "^2.0.0",
    "mocha": "^3.2.0",
    "nock": "^8.0.0",
    "nyc": "^10.2.0",
    "react-addons-perf": "^15.3.1",
    "react-addons-test-utils": "^15.6.2",
    "react-hot-loader": "^1.3.1",
    "redux-mock-store": "^1.5.1",
    "redux-thunk": "^2.0.1",
    "redux": "^4.0.0",
    "react-dom": "^16.3.2"
  },
  "dependencies": {
    "react": "^16.3.2",
    "react-redux": "^5.0.7"
  }
}
